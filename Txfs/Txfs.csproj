<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>spikensbror</Authors>
    <Description>TXFS (transactable file system) is an open-source .NET library that allows transactional file system access.</Description>
    <PackageProjectUrl>https://github.com/spikensbror-dotnet/txfs</PackageProjectUrl>
    <RepositoryUrl>https://github.com/spikensbror-dotnet/txfs</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
  </ItemGroup>

</Project>
